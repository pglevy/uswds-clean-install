{
  "name": "uswds-clean-install",
  "version": "1.0.0",
  "description": "Working installation of USWDS you can view on Github Pages or edit on Gitpod",
  "main": "index.md",
  "scripts": {
    "postinstall": "npm run jekyll:install",
    "jekyll:install": "gem install bundler && bundle install",
    "jekyll:serve": "bundle exec jekyll serve --host=0.0.0.0",
    "start": "npm run jekyll:serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pglevy/uswds-clean-install.git"
  },
  "keywords": [
    "uswds",
    "github-pages",
    "gitpod"
  ],
  "author": "Philip Levy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pglevy/uswds-clean-install/issues"
  },
  "homepage": "https://github.com/pglevy/uswds-clean-install#readme",
  "dependencies": {
    "@uswds/uswds": "^3.1.0"
  },
  "devDependencies": {
    "@uswds/compile": "^1.0.0-beta.3"
  }
}
